    <resource>org.lilie.services.eliot.tice.notes.DernierChangementDansClasse</resource>
    <resource>org.lilie.services.eliot.tice.notes.DirtyMoyenne</resource>
    <resource>org.lilie.services.eliot.tice.notes.Evaluation</resource>
    <resource>org.lilie.services.eliot.tice.notes.InfoCalculMoyennesClasse</resource>
    <resource>org.lilie.services.eliot.tice.notes.ModeleAppreciation</resource>
