    <resource>org.lilie.services.eliot.tice.notes.ResultatEleveServicePeriode</resource>
    <resource>org.lilie.services.eliot.tice.notes.ResultatEleveSousServicePeriode</resource>
    <resource>org.lilie.services.eliot.tice.notes.TypeAppreciationEnum</resource>
    <resource>org.lilie.services.eliot.tice.notes.TypeMoyenneEnum</resource>
