<plugin name='eliot-notes-plugin' version='2.0.0-SNAPSHOT' grailsVersion='2.0.3 &gt; *'>
  <author>Franck Silvestre - Fylab</author>
  <title>Eliot Notes  Plugin</title>
  <description>Plugin contenant les services métiers relatifs à la gestion des notes"
</description>
  <type>EliotNotesPluginGrailsPlugin</type>
  <resources>
    <resource>org.lilie.services.eliot.tice.notes.AppreciationClasseEnseignementPeriode</resource>
